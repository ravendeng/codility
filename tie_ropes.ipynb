   "source": [
    "def solution(K, A):\n",
    "    cnt = 0\n",
    "    current = 0\n",
    "    for part in A:\n",
    "        current += part\n",
    "        if current >= K:\n",
    "            cnt +=1\n",
    "            current = 0\n",
    " \n",
    "    return cnt"
   ]
